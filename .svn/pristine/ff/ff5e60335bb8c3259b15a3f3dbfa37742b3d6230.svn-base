#container{padding: 0 14px}

.cont1,
.cont2,
.cont3,
.swiper-container{margin-bottom:4px!important}

/*
.cont1 .img{float:left;width: 67%;}
.cont1 .img img{width:100%;}
.cont1 .floatR{width: 32%;display: inline-block}
.cont1 .floatR img{width: 100%; }
*/
.cont1 a{width: 49%;}
.cont1 a img{width: 100%;}

.cont2{border: 1px solid #5c6d7f;border-radius: 3px;background:#fff;padding:20px 20px;overflow: hidden;}
.cont2 .floatL{width: 50%;}
.cont2 .floatL img{width: 100%;}
.cont2 .floatR{width: 50%;}
.cont2 .floatR img{width: 100%;padding-top:4px}

.cont3 a{width: 49%;}
.cont3 a img{width: 100%;}

.swiper-container{position: relative;overflow: hidden;}
.swiper-slide{text-align:center;display:-webkit-box;display:-ms-flexbox;display:-webkit-flex;display:flex;-webkit-box-pack:center;-ms-flex-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-ms-flex-align:center;-webkit-align-items:center;align-items:center;width: 33.3%;float:left;text-align: center;}
.swiper-slide img{width:98%;height: 98%;}

#footer{ overflow:hidden; background-color:#292929; position:fixed; bottom:0; left:0; width:100%; height:49px; z-index:9999;clear: both;}
.foot li{ float:left; width:25%;}
.foot li a{ display:block; text-indent:-9999px; height:49px; line-height:49px; text-align:center; margin:0 auto;}
.foot li.foot1 a{ background:url(../../images/foot1.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot2 a{ background:url(../../images/foot2.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot3 a{ background:url(../../images/foot3.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot4 a{ background:url(../../images/foot4.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}

.Button.big{padding:0 20px!important}
.Button.red{background:#ea002c!important;border:none!important;font-size:1.2em;color:#fff;font-weight:bold;border-radius:5px;}
.Button.red2{background:#fff!important;border:1px solid #ea002c;font-size:1.2em;color:#ea002c;font-weight:bold;border-radius:5px;}
.Button.red21{background:#fff!important;border:1px solid #ea002c;font-size:1.2em;color:#ea002c;font-weight:bold;border-radius:5px;width:45%;}
.Button.blue{background:#455668!important;border:none!important;font-size:1.2em;color:#fff;font-weight:bold;border-radius:5px;}
.Button.blue2{background:#455668!important;border-radius:30px;border-color:#455668;font-size:1.125em;color:#fff;font-weight:bold;height: 20px;line-height:24px}

