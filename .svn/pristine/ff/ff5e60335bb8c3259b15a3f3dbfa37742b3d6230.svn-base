body {background:#d5e4f5 url(../../images/bg_main.png) no-repeat 50% 0;background-size:360px 360px;font-size: 0.75em }

#wrap{margin:0 auto;padding-bottom:60px}

#header {position: relative;height:50px}
#header h1 {position: absolute; top:13px; left: 14px;}
#header h1 a{display: block; height: 29px; text-indent: -9999px; }
#header h1.guest a {background: url(../../images/logo_guest.png) 0 0 no-repeat;background-size: 115px 28px;width: 115px;}
#header h1.koone a {background: url(../../images/logo_koone.png) 0 0 no-repeat;background-size: 214px 29px;width: 214px;}
#header h1.bs a {background: url(../../images/logo_bs.png) 0 0 no-repeat;background-size: 173px 29px;width:173px}
#header h1.ch a {background: url(../../images/logo_ch.png) 0 0 no-repeat;background-size: 200px 29px;width:200px}
#header h1.gumi a {background: url(../../images/logo_gumi.png) 0 0 no-repeat;background-size: 238px 29px;width:238px}
#header h1.gw a {background: url(../../images/logo_gw.png) 0 0 no-repeat;background-size: 167px 29px;width:167px}
#header h1.jb a {background: url(../../images/logo_jb.png) 0 0 no-repeat;background-size: 201px 29px;width:201px}
#header h1.jn a {background: url(../../images/logo_jn.png) 0 0 no-repeat;background-size: 182px 29px;width:182px}
#header h1.pohang a {background: url(../../images/logo_pohang.png) 0 0 no-repeat;background-size: 238px 29px;width:238px}
#header .btn_set {position: absolute;top: 16px;right: 16px;}
#header #slidx_button {background: url(../../images/btn_set.png) 50% 50% no-repeat; background-size: 22px 22px; width: 22px; height: 22px; display: block;text-indent: -9999px;border: none;}
#header #slidx_button:hover {cursor: pointer}
#header .btn_more{position:absolute;top:16px;right:46px}
#header #more_button{background:url(../../images/btn_more.png) 50% 50% no-repeat;background-size:24px 21px; width:24px; height:21px; display:block; text-indent:-999em; border:none}

