html, body {height: 100%;}
body {background:#d5e4f5 url(../../images/bg_main.png) no-repeat 50% 0;background-size:360px 360px;font-size: 0.75em }

#wrap{margin:0 auto;padding-bottom:60px}

#header {position: relative;height:50px}
#header h1 {position: absolute; top:13px; left: 14px;}
#header h1 a{display: block; height: 29px; text-indent: -9999px; }
#header h1.koone a {background: url(../../images/logo_koone.png) 0 0 no-repeat;background-size: 214px 29px;width: 214px;}
#header h1.bs a {background: url(../../images/logo_bs.png) 0 0 no-repeat;background-size: 173px 29px;width:173px}
#header h1.ch a {background: url(../../images/logo_ch.png) 0 0 no-repeat;background-size: 200px 29px;width:200px}
#header h1.gumi a {background: url(../../images/logo_gumi.png) 0 0 no-repeat;background-size: 238px 29px;width:238px}
#header h1.gw a {background: url(../../images/logo_gw.png) 0 0 no-repeat;background-size: 167px 29px;width:167px}
#header h1.jb a {background: url(../../images/logo_jb.png) 0 0 no-repeat;background-size: 201px 29px;width:201px}
#header h1.jn a {background: url(../../images/logo_jn.png) 0 0 no-repeat;background-size: 182px 29px;width:182px}
#header h1.pohang a {background: url(../../images/logo_pohang.png) 0 0 no-repeat;background-size: 238px 29px;width:238px}


/*
#header h2 { font-family:'Moebius','Moebius Korea';font-size:1.5em;height: 50px; line-height: 50px;margin: 0 0 0 65px;color:#333}
*/
#header .btn_set {position: absolute;top: 16px;right: 16px;}
#header #slidx_button {background: url(../../images/btn_set.png) 50% 50% no-repeat; background-size: 22px 22px; width: 22px; height: 22px; display: block;text-indent: -9999px;border: none;}
#header #slidx_button:hover {cursor: pointer}
#header .btn_more{position:absolute;top:16px;right:46px}
#header #more_button{background:url(../../images/btn_more.png) 50% 50% no-repeat;background-size:24px 21px; width:24px; height:21px;display:block;text-indent:-999em;border:none}


#container{padding: 0 14px}

.cont1,
.cont2,
.cont3,
.swiper-container{margin-bottom:4px!important}

.cont1{display: block;}
.cont1 .floatL{width: 68%;border:1px solid #4953c2;border-radius:3px;background:#656ee5 url(../../images/bg_cont1_l.png) no-repeat 90% 50%;background-size: 95px 80px;position:relative;min-height:139px}
.cont1 .floatL div{display: table-cell;height: auto;color:#fff;text-align: center;}
.cont1 .floatL h3{font-weight:bold;font-size:1.375em;position: absolute; top: 10%;margin-left:-70px;width: 80px;left:50%;padding: 8px 18px 0 18px;}
.cont1 .floatL h3.new{background:url(../../images/ico_new.png) no-repeat 100% 0;background-size:16px 16px;display:inline-block;}
.cont1 .floatL .dday{display: block;position: absolute; top: 40%;left:50%;margin-left:-30px;font-size:1.125em}
.cont1 .floatL .dday span{display: inline-block;background:#ea002c;border:1px solid #ea002c;border-radius:5px;padding:4px 6px;font-weight:bold;color:#fff;font-size:1.125em}
.cont1 .floatL .date {display: block;position: absolute; bottom: 10%;left:50%;width: 170px;margin-left:-95px;line-height:160%;font-weight:bold}
.cont1 .floatL .date span{color:#f0dfff}
.cont1 .floatL .notime {display: block;position: absolute; top: 44%;left:50%;width: 116px;margin-left:-95px;line-height:160%;font-weight:bold;color:#fdff4d;text-align:right}

.cont1 .floatR{width: 30%;border:1px solid #374352;border-radius:3px;background:#455668 url(../../images/bg_cont1_r.png) no-repeat 50% 47%;background-size: 45px 43px;position:relative;min-height:139px;}
.cont1 .floatR div{display: table-cell;height: auto;color:#fff;text-align: center;}
.cont1 .floatR h3{font-weight:bold;font-size:1.375em;position: absolute; top: 10%;margin-left:-40px;width: 80px;left:50%;color:#a3bad7;padding-top:8px}
.cont1 .floatR p{display: block;position: absolute; bottom: 10%;left:50%;width: 120px;margin-left:-60px;line-height:140%;font-weight:bold;letter-spacing:-1px;font-size:11px;}



/* ����Ʈ�� ����+���� */
@media only screen and (min-device-width : 320px) and (max-device-width : 480px){
.cont1 .floatL{background-size: 95px 80px;}
.cont1 .floatR{background-size: 45px 43px;}

}

 /* ����Ʈ�� ���� */
@media only screen and (min-width : 321px) {
	.cont1 .floatL{background-size:80px 70px}
.cont1 .floatR{background-size: 45px 43px;}
}

 /* ����Ʈ�� ���� */
@media only screen and (max-width : 320px) {
	.cont1 .floatL{background-size:80px 70px}
.cont1 .floatR{background-size: 45px 43px;}
}

