.cont2{border: 1px solid #5c6d7f;border-radius: 3px;background:#fff url(../../images/bg_mypage.png) no-repeat 20px 90%;background-size:96px 53px;padding:12px 10px 10px 130px;position: relative;}
.cont2 h3{font-weight:bold;font-size:1.375em;position: absolute; top: 10px; left: 20px;letter-spacing:-1px}
.cont2 p{background: url(../../images/ico_notice.png) no-repeat 0 5px;background-size:12px 11px;padding: 0 0 10px 20px;font-size:14px;letter-spacing:-1px;margin:10px 0;line-height:150%}
.cont2 ul li {background: url(../../images/ico_notice.png) no-repeat 0 2px;background-size:12px 11px;padding: 0 0 6px 20px;font-size:14px;letter-spacing:-1px}
.cont2 ul li span.ltt{width:70px;display: inline-block;}
.cont2 ul li span.rtt{color:#df4900}

/*
.cont3 a{width: 49%;}
.cont3 a img{width: 100%;}
*/

.cont3 .floatL{width: 49%;border:1px solid #2b8bbb;border-radius:3px;background:#35a6d1 url(../../images/bg_cont3_l.png) no-repeat 50% 80%;background-size: 82px 55px;position:relative;min-height:139px}
.cont3 .floatL div{display: table-cell;height: auto;color:#fff;text-align: center;}
.cont3 .floatL h3{font-weight:bold;font-size:1.375em;position: absolute; top: 10%;margin-left:-74px;width:110px;left:50%;padding:8px 18px 0 18px}
.cont3 .floatL h3 a{color:#fff}
.cont3 .floatL h3.new{background:url(../../images/ico_new.png) no-repeat 100% 0;background-size:16px 16px;display:inline-block;margin-left:-80px}
.cont3 .floatL .bill{background:url(../../images/ico_bill.png) no-repeat 0 3px;background-size:13px 13px;display:inline-block;padding: 0 16px;position: absolute; top:30%;left:50%;margin-left:-60px;line-height:160%;font-weight:bold;letter-spacing:-1px}
.cont3 .floatL .bill.view{margin-left:-40px;}
.cont3 .floatL .bill a{color:#def6ff}
.cont3 .floatR{width: 49%;border:1px solid #2867c2;border-radius:3px;background:#4a89da;position:relative;min-height:139px}
.cont3 .floatR div{display: table-cell;height: auto;color:#fff;text-align: center;}
.cont3 .floatR h3{font-weight:bold;font-size:1.375em;position: absolute; top: 10%;margin-left:-80px;padding-top:8px;width:160px;left:50%;line-height:120%}
.cont3 .floatR h3 span{display:block;font-size:0.75em;color:#e1eeff}
.cont3 .floatR .tell{background:url(../../images/bg_cont3_r.png) no-repeat 0 3px;background-size:23px 30px;display:inline-block;padding-left:28px;position: absolute; top:60%;left:50%;margin-left:-70px;line-height:160%;font-weight:bold;letter-spacing:-1px;text-align: left;line-height:150%}
.cont3 .floatR .tell span{display: block;}
.cont3 .floatR .col_yellow{font-size:1.4em}
.cont3 .floatR .time{font-size:1.0em;color:#000}

 
.swiper-container{position: relative;overflow: hidden;}
.swiper-slide{text-align:center;display:-webkit-box;display:-ms-flexbox;display:-webkit-flex;display:flex;-webkit-box-pack:center;-ms-flex-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-ms-flex-align:center;-webkit-align-items:center;align-items:center;}
.swiper-slide img{width:98%;height: 100%;outline:none}

.notice {border: 1px solid #8090a3;position: relative;padding: 0 0 0 90px;border-radius: 3px;background-color:#f0f0f0;margin-bottom:10px}
.notice:before {content:"NOTICE";font-size:14px;display: inline-block;font-style: normal;background: #324ba6;padding: 3px 10px;color: #FFF;position: absolute;top:7px;left:9px;border-radius:30px}
.notice:after {content:'';display: block;top: 0;left: 90px;height: 20px;}
.notice ul li {list-style: none;padding: 9px 0 11px;height:12px;line-height:12px}
.notice ul li a{text-decoration:none;color:#333;}
.notice ul li a span{text-decoration:none;color:#80573f}

.used h3{font-weight:bold;font-size: 1.25em;float:left;}
.used .list_dot{float:right;text-align: right;}
.used .list_dot li .dot{background: url(../../images/bu_dot.png) no-repeat 0 4px;background-size:4px 4px;display:inline-block;padding: 0 0 4px 10px;clear: both;float: none;}
.used .list_dot li .col_red{color:#ea002c}

#footer{ overflow:hidden; background-color:#292929; position:fixed; bottom:0; left:0; width:100%; height:49px; z-index:9999;clear: both;}
.foot li{ float:left; width:25%;}
.foot li a{ display:block; text-indent:-9999px; height:49px; line-height:49px; text-align:center; margin:0 auto;}
.foot li.foot1 a{ background:url(../../images/foot1.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot2 a{ background:url(../../images/foot2.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot3 a{ background:url(../../images/foot3.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}
.foot li.foot4 a{ background:url(../../images/foot4.png) 50% 0 no-repeat; border-left:none; background-size:90px 49px;}

#load{width: 100%;height: 100%;background:#444dcb}
#load img{width: 100%; }

.Button.big{padding:0 20px!important}
.Button.red{background:#ea002c!important;border:none!important;font-size:1.2em;color:#fff;font-weight:bold;border-radius:5px;}
.Button.red2{background:#fff!important;border:1px solid #ea002c;font-size:1.2em;color:#ea002c;font-weight:bold;border-radius:5px;}
.Button.red21{background:#fff!important;border:1px solid #ea002c;font-size:1.2em;color:#ea002c;font-weight:bold;border-radius:5px;width:45%;}
.Button.blue{background:#455668!important;border:none!important;font-size:1.2em;color:#fff;font-weight:bold;border-radius:5px;}
.Button.blue2{background:#455668!important;border-radius:30px;border-color:#455668;font-size:1.125em;color:#fff;font-weight:bold;height: 20px;line-height:24px}
