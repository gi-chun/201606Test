// 배너롤링/
$('.swipe1').click(function(ss){
	clearTab(1);
	ss.target.src = '../../images/tab_app1_on.png';
	$('#tab1').show();
});

$('.swipe2').click(function(ss){
	clearTab(2);
	ss.target.src = '../../images/tab_app2_on.png';
	$('#tab2').show();
});

$('.swipe3').click(function(ss){
	clearTab(3);
	ss.target.src = '../../images/tab_app3_on.png';
	$('#tab3').show();
});

$('.swipe4').click(function(ss){
	clearTab(4);
	ss.target.src = '../../images/tab_app4_on.png';
	$('#tab4').show();
});

function clearTab(no){
	for(var i=1;i<6;i++){
		if(i==no){
		}else{

			$('.swipe'+i).children().children().attr('src','../../images/tab_app'+i+'_off.png');
			$('#tab'+i).hide();

			
		}
	}
}

$(function(){	
	var swiper = new Swiper('.swiper-container', {
		nextButton: '.swiper-button-next', 
		prevButton: '.swiper-button-prev',
		slidesPerView: 4,
		paginationClickable: true,
		spaceBetween: 0
	});
	
});

$a.page(function(e,a){
	this.init = function(e){
		$('.swipe1').children().children().attr('src','../../images/tab_app1_on.png');
		$('#tab1').show();
	};
});
//]]>
