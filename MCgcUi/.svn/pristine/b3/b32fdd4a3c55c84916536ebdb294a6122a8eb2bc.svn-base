.box_type1 .btn_input_num{position: absolute; top: 14px; right: 10px;}
.box_type1 #button_input_num {background: url(../../images/btn_input_num.png) 50% 50% no-repeat; background-size: 16px 15px; width: 16px; height: 15px; display: block;text-indent: -9999px;border: none;}

.input{float:right;padding:0 10px;border-radius:30px;}
.input.input_no{background-color:#fff;border:1px solid #ea002c;color:#ea002c;}
.input.input_date{background-color:#fff;border:1px solid #333;color:#333;}
.input.input_closed{background-color:#999;border:1px solid #999;color:#fff;}

.box_type2{border:1px solid #9fb3ca;border-radius:3px;background-color:#fff;overflow: hidden;font-size:1.0em;position: relative;line-height:20px;font-weight:bold;margin-bottom:10px}
