.box_poll .tac {text-align: center;font-size:0.875em }
.box_poll .inner .textarea {background-color:#f1f4f7;border-radius:3px;border:1px solid #cfd1e2;height: 60px;line-height:24px;padding:10px;margin-top:10px} 
