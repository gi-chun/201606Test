.box_poll{border:1px solid #9fb3ca;border-radius:3px;background-color:#fff;overflow: hidden;line-height:20px;position: relative;margin:3px 0}
.box_poll h2{background-color:#f6f6f4;border-bottom:1px solid #d9e0e7;border-top-left-radius:5px;border-top-right-radius:5px;padding:10px 50px 10px 16px;color:#214085;font-size:1.4em;font-weight:bold;letter-spacing:-1px}
