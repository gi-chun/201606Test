.box_type2 ul:last-child{border-bottom:none}
.box_type2 ul li{font-size:1.2em;color:#333;position: relative;padding-left:94px}
.box_type2 ul li strong{position: absolute; top: 0; left: 0;width: 80px;color:#444;background: url(../../images/bu_dot_sky.png) no-repeat 0 50%;background-size:4px 4px;padding-left:12px;letter-spacing:-1px;font-size:0.9em}
.box_type2 ul li.none{padding-left:0;float:none}
.box_type2 ul li .floatR{color:#c10024;font-size:0.8em;}
.box_type2 .d-day{background-color:#ea002c;border:1px solid #ea002c;border-radius:5px;color:#fff;padding:1px 5px;line-height:14px;height:16px;display:inline-block}

.box_type3{padding:8px 10px;border:1px solid #3c4385;border-radius:3px;background-color:rgba(73, 77, 127, 0.2);overflow: hidden;font-weight:bold;position: relative;margin-bottom:10px;position: relative;}
.box_type3 ul li {position: relative;padding: 0 0 6px 90px;font-size:1.125em}
.box_type3 ul li.tit strong {color:#000;font-size:1.125em;}
.box_type3 ul li.tit .col_red {font-size:1.4em;}
.box_type3 li strong{position: absolute; top: 0; left: 0;}
.box_type3 li strong{color:#444}

.box_info{border:1px solid #9fb3ca;border-radius:3px;background-color:#fff;overflow: hidden;}
.box_info dt{background:#eaf2fa url(../../images/bu_info.png) no-repeat 16px 14px;background-size:16px 16px;padding:14px 10px 16px 40px;font-size:1.2em;color:#333;font-weight:bold;border-bottom:1px solid #9fb3ca}
.box_info dd{background-color:#e6edf4;padding:16px 20px;line-height:16px;font-size:1.0em}
.box_info dd .col_blue{font-size:1.2em;font-weight:bold}

.box_join{border:1px solid #9fb3ca;border-radius:3px;background-color:#fff;overflow: hidden;line-height:20px;position: relative;margin:3px 0}
.box_join h2{background-color:#f6f6f4;border-bottom:1px solid #9fb3ca;border-top-left-radius:5px;border-top-right-radius:5px;padding:10px 16px;color:#214085;font-size:1.4em;font-weight:bold;letter-spacing:-1px}
.box_join .btn{position: absolute; top: 3px; right: 5px;}
.box_join .inner{padding:10px 16px}
.box_join .inner .img{padding:10px 0;border-top:1px solid #dce3ec;border-bottom:1px solid #dce3ec;text-align: center;}
.box_join .inner .img img{width: 70%;max-width:636px}
.box_join .input_phone{padding:10px 0;border-top:1px solid #dce3ec;border-bottom:1px solid #dce3ec}
.box_join .input_phone p{position: relative;padding-left:120px;}
.box_join .input_phone p.tar{padding:0}
/* 2015-07-06 
	.box_join .input_phone label 수정
	.box_join .input_phone strong 추가
*/
.box_join .input_phone label{font-weight:bold;font-size:1.125em;display: inline-block;position: absolute; top: 0; left: 0;color:#333}
.box_join .input_phone strong{font-weight:bold;font-size:1.125em;display: inline-block;position: absolute; top: 0; left: 0;color:#333}
.box_join .input_phone .input_txt{width: 100%;background-color:#f1f4f7;border:1px solid #cfd1e2;border-radius:3px;height: 24px;line-height:24px;font-size:1.125em;}
.box_join .input_phone .txt{width: 100%;line-height:24px;font-size:1.25em;font-weight:bold}

.box_join .bill{padding:10px 0;border-top:1px solid #dce3ec;}
.box_join .bill p{position: relative;padding:0 0 10px 80px;overflow: hidden;}
.box_join .bill strong{font-weight:bold;font-size:1.125em;display: inline-block;position: absolute; top: 0; left: 0;color:#333}
.box_join .bill .bold{font-size:1.25em}
