.box_poll .paging{background-color:#a1a9b2;width:100%;height: 13px;margin-bottom:10px}
.box_poll .paging span {background: url(../../images/bg_poll.png) repeat-x; background-size: auto;display: block;text-indent: -9999px;border: none;height: 13px;}
