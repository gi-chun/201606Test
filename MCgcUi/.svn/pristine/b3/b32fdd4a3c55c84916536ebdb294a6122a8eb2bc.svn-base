body {font-size: 0.75em;background-color:#d5e4f5;line-height:20px;color:#444}

#wrap{margin:0 auto;padding-bottom:80px;}

#header {background-color:#656ee5;border-bottom:1px solid #032b62;position: relative;}
#header h1.title { font-family:'Moebius','Moebius Korea';font-size:1.5em;height: 50px; line-height: 50px;text-align: center;}
#header h1.title a{text-decoration:none;color:#fff;}
#header .btn_home {position: absolute;top: 14px;left:16px;}
#header .btn_noUsrHome {position: absolute;top: 14px;left:16px;}
#header .btn_back {position: absolute;top: 14px;left:16px;}
#header .btn_set {position: absolute;top: 14px;right: 16px;}
#header #button_home {background: url(../../images/btn_home.png) 50% 50% no-repeat; background-size: 22px 22px; width: 22px; height: 22px; display: block;text-indent: -9999px;border: none;}
#header #button_back {background: url(../../images/btn_close.png) 50% 50% no-repeat; background-size: 22px 22px; width: 22px; height: 22px; display: block;text-indent: -9999px;border: none;}
#header #button_set {background: url(../../images/btn_set_w.png) 50% 50% no-repeat; background-size: 22px 22px; width: 22px; height: 22px; display: block;text-indent: -9999px;border: none;}
#header #button_home:hover,
#header #button_set:hover {cursor: pointer}

#container{background:#d5e4f5 url(../../images/bg_main.png) no-repeat 50% 0;background-size:360px 360px;}
#container .content{padding: 0 14px}
#container h2.ko-one{background: url(../../images/logo_koone.png) 0 0 no-repeat; display: block; background-size: 214px 29px;  width: 214px;  height: 29px; text-indent: -9999px;margin:10px auto}
#container h2.koone{background: url(../../images/logo_koone.png) 0 0 no-repeat; display: block; background-size: 214px 29px;  width: 214px;  height: 29px; text-indent: -9999px;margin:10px auto}

#container h2.bs {background: url(../../images/logo_bs.png) 0 0 no-repeat;background-size: 173px 29px;width:173px;  height: 29px; text-indent: -9999px;margin:10px auto}
#container h2.ch {background: url(../../images/logo_ch.png) 0 0 no-repeat;background-size: 200px 29px;width:200px;  height: 29px; text-indent: -9999px;margin:10px auto}
#container h2.gumi {background: url(../../images/logo_gumi.png) 0 0 no-repeat;background-size: 238px 29px;width:238px;  height: 29px; text-indent: -9999px;margin:10px auto}
#container h2.gw {background: url(../../images/logo_gw.png) 0 0 no-repeat;background-size: 167px 29px;width:167px;  height: 29px; text-indent: -9999px;margin:10px auto}
#container h2.jb {background: url(../../images/logo_jb.png) 0 0 no-repeat;background-size: 201px 29px;width:201px;  height: 29px; text-indent: -9999px;margin:10px auto}
